package pro;

public class Pattern24 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=1;i<10;i=i+2) {
			for(int j=1;j<=i;j++) {
				System.out.print(j);
				
			}
			System.out.println();
		}
		for(int i=9;i>=1;i=i-2) {
			for(int j=1;j<=i;j++) {
				System.out.print(j);
			}
			System.out.println();
		}
	}

}
