package pro;

public class Pattern26 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N=5;
		int count=1;
		for(int i=1;i<=N;i++) {
			int flag=-1;
			
			for(int j=1;j<=i;j++) {
				if(flag==1) System.out.print("*");
				System.out.print(count);
				flag=1;
				count++;
			}
			System.out.println();
		}
	}

}
