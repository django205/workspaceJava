package pro;

public class Pattern26 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N=5;
		int count1=1;
		int count2=1;
		for(int i=1;i<=N;i++) {
			count2=count1+i;
			int flag=-1;
			for(int j=1;j<=i;j++,count1++) {
				if(flag==1) System.out.print("*");
				flag=1;
				if(i%2==0) System.out.print(--count2);
				else System.out.print(count1); 
				
			}
			System.out.println();
		}
	}

}
