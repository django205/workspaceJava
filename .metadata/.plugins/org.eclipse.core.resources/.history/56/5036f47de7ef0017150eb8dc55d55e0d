package pro;

public class Pattern29 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N=4;
		for(int i=1;i<=N;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(i+2);
			}
			for(int k=N;k>=i;k--) System.out.print(k+2);
			System.out.println();
		
		}
	}

}
