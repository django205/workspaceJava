package pro;

public class Pattern29 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int N=4;
		for(int i=1;i<=N;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(i+2);
			}
			System.out.println();
		
		}
		for(int i=N;i>=1;i--) {
			
			for(int k=1;k<=i;k++) System.out.print(i+2);
			System.out.println();
		}
	}

}
